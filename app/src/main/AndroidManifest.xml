<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dulanjali.agroworld">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_spa_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_spa_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity_Login" />
        <activity android:name=".Activity_Register" />
        <activity android:name=".Activity_Home"></activity>
    </application>

</manifest>